{
  "name": "quickship-cli",
  "version": "1.0.1",
  "description": "Ship production-ready web, mobile, and backend projects in 60 seconds. Create Next.js, React, T3 Stack, MERN, Expo React Native, Express, or NestJS apps with TypeScript, flexible database options (Prisma/Mongoose or raw drivers), and modern tooling - then deploy instantly to Vercel or Netlify.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "quickship": "./bin/index.js"
  },
  "scripts": {
    "dev": "node bin/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.js bin/**/*.js",
    "lint:fix": "node node_modules/eslint/bin/eslint.js src/**/*.js bin/**/*.js --fix",
    "format": "node node_modules/prettier/bin/prettier.cjs --write \"src/**/*.js\" \"bin/**/*.js\"",
    "format:check": "node node_modules/prettier/bin/prettier.cjs --check \"src/**/*.js\" \"bin/**/*.js\"",
    "prepublishOnly": "npm run format:check"
  },
  "keywords": [
    "cli",
